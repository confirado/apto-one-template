{
  "name": "apto-angular-template",
  "version": "0.0.0",
  "scripts": {
    "build-release": "npm run live-frontend && npm run staging-frontend",
    "cypress:open": "APTO_CYPRESS_ENV=dev node_modules/.bin/cypress open",
    "cypress:run": "APTO_CYPRESS_ENV=dev node_modules/.bin/cypress run",
    "apto-start-backend": "npm run apto-build && ng serve backend --host grobi.projektversion.de --port 4200",
    "apto-start-frontend": "npm run apto-build && ng serve frontend --host grobi.projektversion.de --port 4201",
    "local-frontend": "npm run apto-build && ng build frontend --configuration production",
    "local-frontend-dev": "npm run apto-build && ng build frontend --watch --configuration development",
    "live-frontend": "npm run apto-build && ng build frontend --configuration production,live",
    "live-frontend-dev": "npm run apto-build && ng build frontend --watch --configuration development,live-dev",
    "staging-frontend": "npm run apto-build && ng build frontend --configuration production,staging",
    "staging-frontend-dev": "npm run apto-build && ng build frontend --watch --configuration development,staging-dev",
    "install-frontend": "ng build install --configuration production",
    "install-frontend-dev": "ng build install --watch --configuration development",
    "apto-build": "node ./apto-build.mjs",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/color-picker": "~8.0.0",
    "@angular-material-components/file-input": "~8.0.0",
    "@angular/animations": "^14.2.2",
    "@angular/common": "^14.2.2",
    "@angular/compiler": "^14.2.2",
    "@angular/core": "^14.2.2",
    "@angular/forms": "^14.2.2",
    "@angular/material": "^14.2.2",
    "@angular/platform-browser": "^14.2.2",
    "@angular/platform-browser-dynamic": "^14.2.2",
    "@angular/router": "^14.2.2",
    "@ngneat/until-destroy": "^9.2.2",
    "@ngrx/effects": "^14.3.1",
    "@ngrx/store": "^14.3.1",
    "angular-resize-event": "^3.2.0",
    "bignumber.js": "^9.1.2",
    "decimal.js": "^10.4.3",
    "fabric": "^5.3.0",
    "fontfaceobserver": "^2.1.0",
    "mathjs": "^11.11.1",
    "ngx-pipes": "^3.2.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.3",
    "@angular/cli": "^14.2.3",
    "@angular/compiler-cli": "^14.2.2",
    "@ngrx/store-devtools": "^14.3.1",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "cypress": "^13.3.1",
    "eslint": "^8.23.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.26.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "recursive-readdir-synchronous": "^0.0.4",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.8.3"
  }
}
